import java.util.Scanner;

public class Tic_Tac_Toe {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		char[][] board = new char[3][3];
		
	}

}

class board{
	private static char[][] setBoard=new char[3][3];
	
	//게임판 초기화
	public void init(){
		for(int i=0; i<3; i++)
			for(int j=0; j<3; j++)
				this.setBoard[i][j] = ' ';
	}
	
	//입력좌표 유효확인
	public boolean check_ox(int x, int y){
		if(this.setBoard[x][y]!=' '){
			return false;
		}
		
		return true;
	}
	
	//결과 확인
	public boolean check_win(){
		if(this.setBoard[0][0]=='o'){
			if(this.setBoard[1][0]=='o'){
				if(this.setBoard[2][0]=='o'){
					return true;
				}
			}
			if(this.setBoard[1][1]=='o'){
				if(this.setBoard[2][2]=='o'){
					return true;
				}
			}
		}
		if(this.setBoard[0][1]=='o'){
			if(this.setBoard[1][1]=='o'){
				if(this.setBoard[2][1]=='o'){
					return true;
				}
			}
		}
		if(this.setBoard[0][2]=='o'){
			if(this.setBoard[1][2]=='o'){
				if(this.setBoard[2][2]=='o'){
					return true;
				}
			}
		}
		//////////////////////
		if(this.setBoard[0][0]=='o'){
			if(this.setBoard[0][1]=='o'){
				if(this.setBoard[0][2]=='o'){
					return true;
				}
			}
			if(this.setBoard[1][1]=='o'){
				if(this.setBoard[2][0]=='o'){
					return true;
				}
			}
		}
		if(this.setBoard[1][0]=='o'){
			if(this.setBoard[1][1]=='o'){
				if(this.setBoard[1][2]=='o'){
					return true;
				}
			}
		}
		if(this.setBoard[2][0]=='o'){
			if(this.setBoard[2][1]=='o'){
				if(this.setBoard[2][2]=='o'){
					return true;
				}
			}
		}
		///////////////
		///////////////
		if(this.setBoard[0][0]=='x'){
			if(this.setBoard[1][0]=='x'){
				if(this.setBoard[2][0]=='x'){
					return true;
				}
			}
			if(this.setBoard[1][1]=='x'){
				if(this.setBoard[2][2]=='x'){
					return true;
				}
			}
		}
		if(this.setBoard[0][1]=='x'){
			if(this.setBoard[1][1]=='x'){
				if(this.setBoard[2][1]=='x'){
					return true;
				}
			}
		}
		if(this.setBoard[0][2]=='x'){
			if(this.setBoard[1][2]=='x'){
				if(this.setBoard[2][2]=='x'){
					return true;
				}
			}
		}
		//////////////////////
		if(this.setBoard[0][0]=='x'){
			if(this.setBoard[0][1]=='x'){
				if(this.setBoard[0][2]=='x'){
					return true;
				}
			}
			if(this.setBoard[1][1]=='x'){
				if(this.setBoard[2][0]=='x'){
					return true;
				}
			}
		}
		if(this.setBoard[1][0]=='x'){
			if(this.setBoard[1][1]=='x'){
				if(this.setBoard[1][2]=='x'){
					return true;
				}
			}
		}
		if(this.setBoard[2][0]=='x'){
			if(this.setBoard[2][1]=='x'){
				if(this.setBoard[2][2]=='x'){
					return true;
				}
			}
		}
		return false;
	}
	
	public void set_user(int x, int y){
		if(check_ox(x,y)){
			this.setBoard[x][y]='x';
		}
	}
	
	public void set_com(){
		if(this.setBoard[1][1]=='o'){
			
		}
		else if(this.setBoard[1][1]=='x'){
			
		}
		else{
			this.setBoard[1][1]='o';
		}
	}
}