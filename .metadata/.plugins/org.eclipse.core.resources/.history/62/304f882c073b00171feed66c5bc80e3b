package thread_example1;

public class MainActivity {
	
	public static void main(String[] args) {
		Thread thread = new Thread01();
		thread.start();
		for(int i = 0; i < 10; i++) {
			System.out.print(i);
		}
		try {
			Thread.sleep(1000);
		} catch(InterruptedException e) {
			System.out.println(e.getMessage());
		}
	}

}
