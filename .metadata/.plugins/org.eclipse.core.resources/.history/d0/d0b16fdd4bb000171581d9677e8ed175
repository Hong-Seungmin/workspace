	<div id="clock"></div>
	<script type="text/javascript">
		function setClock() {
			var now = new Date();
			var s = now.getMonth() + 1 + "¿ù" + now.getDate() + "ÀÏ" + now.getSeconds();

			document.getElementById('clock').innerHTML = s;
			setTimeout('setClock()', 1000);
		}
		setClock();
	</script>
